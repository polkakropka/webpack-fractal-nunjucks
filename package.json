{
  "name": "styleguide-starterkit",
  "version": "0.5.0",
  "description": "A starterkit to create styleguides with Fractal and Webpack.",
  "keywords": [
    "styleguide",
    "toolkit",
    "boilerplate",
    "starterkit",
    "fractal"
  ],
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run clean && npm run build && npm run fractal",
    "build": "npm run build:fractal && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack",
    "build:fractal": "NODE_ENV=production fractal build",
    "release": "release-it",
    "fractal": "fractal start --sync",
    "webpack-server": "webpack-dev-server --hot",
    "node-server": "node ./server.js",
    "clean": "rm -rf ./dist/"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@frctl/fractal": "^1.1.7",
    "@frctl/nunjucks": "^2.0.1",
    "@release-it/conventional-changelog": "^1.1.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.2.0",
    "marked": "^4.0.10",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.3",
    "postcss-loader": "^3.0.0",
    "release-it": "^12.3.5",
    "sass-loader": "^7.3.1",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.0",
    "svgo-loader": "^2.2.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "engines": {
    "node": ">=6 <=10"
  }
}
